{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "games/**,frontend/dist/**"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/sitemap.xml",
      "dest": "api/index.js"
    },
    {
      "src": "/wasm/(.*)/",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "api/index.js"
    },
    {
      "src": "/wasm/(.*)",
      "dest": "/games/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/frontend/dist/assets/$1"
    },
    {
      "src": "/(favicon.svg|homepage-preview.png|main-theme.css|game_shell.css)",
      "dest": "/frontend/dist/$1"
    },
    {
      "handle": "filesystem"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }
      ]
    }
  ]
}
